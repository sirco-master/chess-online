    "react-dom": "^18.2.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "stockfish": "^16.0.0",
    "stockfish.js": "^10.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
